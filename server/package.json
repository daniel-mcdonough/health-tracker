{
  "name": "health-tracker-server",
  "version": "1.0.0",
  "description": "Backend API for health tracking application",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "db:migrate": "node dist/scripts/migrate.js",
    "db:seed": "node dist/scripts/seed.js",
    "generate-password": "node scripts/generate-password-hash.js",
    "generate-mock-data": "tsx src/scripts/generateMockData.ts",
    "test-correlations": "tsx src/scripts/testCorrelations.ts",
    "remove-sleep-data": "tsx src/scripts/removeSleepData.ts",
    "add-sleep-logs": "tsx src/scripts/addSleepLogs.ts",
    "add-ruq-symptom": "tsx src/scripts/addSymptomMigration.ts"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "ml-logistic-regression": "^2.0.0",
    "ml-matrix": "^6.12.1",
    "ml-random-forest": "^2.1.0",
    "sqlite3": "^5.1.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.53.0",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2",
    "vitest": "^1.0.0"
  }
}
